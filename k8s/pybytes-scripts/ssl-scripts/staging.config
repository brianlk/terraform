algorithm="genrsa"
key_bits_length="4096"
days_till_expire=18250

root_common_name="Pybytes CA Cert"
root_password="password"
root_ca_dir="./certificates/staging"
root_ca_prefix="staging.pycom.root.ca"
openssl_root_config_file="./configs/staging.root.cnf"

intermediate_common_name="Pybytes Intermediate CA Cert"
intermediate_password="password"
intermediate_ca_dir="./certificates/staging"
intermediate_ca_prefix="staging.pycom.intermediate"
openssl_intermediate_config_file="./configs/staging.intermediate.cnf"

ca_chain_prefix="staging.pycom.chain.ca"
